node("dev.platoe.io"){
    agent any
    stage("PingSelf") {
        steps {
            sh "ping dev.platoe.io -c 1"
        }
    }
    stage("LastLogin"){
        steps{
            sh '''
            echo "This is the current directory:"
            echo "${PWD}"
            date >> last_login
            '''
        }
    }

}
