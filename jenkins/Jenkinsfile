pipeline{
    agent{ node("dev.platoe.io") }
    stages{
        stage("PingSelf") {
            steps {
                sh "ping dev.platoe.io -c 1"
            }
        }
        stage("CurrentDirectory"){
            steps{
                sh '''
                echo "This is the current directory:"
                pwd
                '''
            }
        }
        stage("Git"){
            steps{
                git credentialsId: 'jenkins-webhook',
                url: 'https://github.com/videap/platoe.git',
                branch: 'dev'
            }
        }
    }
}